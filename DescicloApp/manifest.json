{
	"name": "DescicloApp",
	"description": "Uma extens\u00E3o de Google Chrome, Mozilla Firefox e Opera para acessar a Desciclop\u00E9dia com mais facilidade.",
	"version": "4.5",
	"manifest_version": 2,
	"update_url": "http://aloogle.googlecode.com/svn/trunk/atualizacoes/google_chrome/extensoes/descicloapp.xml",
	"permissions": [ "tabs", "notifications", "contextMenus"],
	"options_page": "paginas/opcoes.html",
	"background": {
	"page":
		"background.html"
	},
	"icons": {
		"16": "icons/icon.ico",
		"32": "icons/icon_32.png",
		"48": "icons/icon_48.png",
		"64": "icons/icon_64.png",
		"128": "icons/icon_128.png"
	},
	"omnibox": { 
		"keyword" : "dp" 
	},
	"homepage_url": "http://desciclopedia.org/wiki/Usu%C3%A1rio:%C3%81s/DescicloApp",
	"browser_action": {
		"default_icon": "icons/icon.png",
		"default_title": "Desciclop\u00E9dia",
		"default_popup": "popup.html"
	}
}
