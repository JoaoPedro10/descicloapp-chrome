{
   "Manifest_version": 2,
   "name": "DescicloApp",
   "description": "Uma extens\u00E3o da Desciclop\u00E9dia para Google Chrome; by \u00C1s http://desciclopedia.org/wiki/Utilizador:\u00C1s ou Aloogle http://code.google.com/p/aloogle",
   "version": "3.1",
   "update_url": "http://aloogle.googlecode.com/svn/trunk/atualizacoes/google_chrome/extensoes/descicloapp.xml",
   "permissions": [ "tabs", "notifications", "http://desciclopedia.org/" ],
