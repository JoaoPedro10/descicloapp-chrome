{
   "background": {
      "page": "background/background.html"
   },
   "browser_action": {
      "default_icon": "icons/icon.png",
      "default_popup": "popup.html",
      "default_title": "Desciclop\u00E9dia"
   },
   "content_scripts": [ {
      "js": [ "lib/jquery.js", "lib/constant.js", "lib/default-config.js", "config/tb-config.js", "config/widget-config.js", "lib/protocol.js", "lib/tb-message.js", "lib/state-machine.js", "lib/window-position.js", "content_script/toolbar.js", "content_script/widget.js", "content_script/injector.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_start"
   }, {
      "css": [ "content_script/hack/facebook.css" ],
      "matches": [ "*://*.facebook.com/*", "*://*.twitter.com/*" ]
   }, {
      "css": [ "content_script/hack/relative.css" ],
      "matches": [ "*://*.google.com/*", "*://*.google.com.br/*", "*://*.ask.com/", "*://*.bagsbuy.com/*", "*://*.csaa.com/*", "*://*.childrenschorus.org/*", "*://*.wikipedia.org/*", "*://*.mercurynews.com/*", "*://*.usnews.com/*", "*://*.desciclopedia.org/*", "*://*.desciclopedia.ws/*", "*://*.pudim.info/*", "*://vist.as/*", "*://pesquisa.la/*", "*://desnoticias.org/*" ],
      "run_at": "document_start"
   }, {
      "css": [ "content_script/hack/static.css" ],
      "matches": [ "*://*.google.com/imgres*", "*://images.google.com/*", "*://codesearch.google.com/*" ],
      "run_at": "document_start"
   } ],
   "description": "Uma extens\u00E3o para Google Chrome, Mozilla Firefox e Opera para acessar a Desciclop\u00E9dia com mais facilidade. Vers\u00E3o 5.0 beta",
   "homepage_url": "http://desciclopedia.org/wiki/DescicloApp",
   "icons": {
      "128": "icons/icon_128.png",
      "16": "icons/icon.ico",
      "32": "icons/icon_32.png",
      "48": "icons/icon_48.png",
      "64": "icons/icon_64.png"
   },
   "manifest_version": 2,
   "name": "DescicloApp",
   "omnibox": {
      "keyword": "dp"
   },
   "options_page": "paginas/opcoes.html",
   "permissions": [ "contextMenus", "management", "notifications", "tabs", "unlimitedStorage", "http://*/*", "https://*/*" ],
   "update_url": "http://aloogle.googlecode.com/svn/trunk/atualizacoes/google_chrome/extensoes/descicloapp.xml",
   "version": "5.0",
   "web_accessible_resources": [ "config/skin/css/containers.css", "config/skin/toolbar.html", "widgets/recentes/recentes.html", "widgets/mensagem/mensagem.html" ]
}
